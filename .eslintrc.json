{
  "extends": "airbnb-base",
  "rules": {
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": 0,
    "import/no-unresolved": [
      "error",
      {
        "ignore": [
          "^vce(\/|$)"
        ]
      }
    ]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "jasmine": true
  }
}